{
  "name": "kafka-express-backend",
  "version": "1.0.0",
  "description": "Express server that reads from Kafka, decodes Solana protobuf messages, and writes to ClickHouse database",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "dev:watch": "nodemon --exec ts-node src/server.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@clickhouse/client": "^1.11.2",
    "@types/protobufjs": "^6.0.0",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "kafkajs": "^2.2.4",
    "protobufjs": "^7.2.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "express",
    "kafka",
    "nodejs",
    "typescript",
    "solana",
    "clickhouse",
    "protobuf"
  ],
  "author": "",
  "license": "ISC"
}
